{
    "name": "snowio/zigzag-data-model",
    "description": "Data model for zigzag",
    "authors": [
        {
            "name": "Alexander Wanyoike",
            "email": "amw@amp.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "SnowIO\\ZigZagDataModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnowIO\\ZigZagDataModel\\Tests\\Unit\\": "tests/unit"
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix --diff src",
        "test:lint": "vendor/bin/php-cs-fixer fix --diff --dry-run src",
        "test:types": "php vendor/bin/phpstan.phar analyze src"
    },
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "friendsofphp/php-cs-fixer": "^2.17@dev"
    }
}
